    "@material/linear-progress": "@material/linear-progress",
    "@material/switch": "@material/switch",
    "@material/tab-indicator": "@material/tab-indicator",
    "@material/tab-scroller": "@material/tab-scroller",
    "@material/circular-progress": "@material/circular-progress",
    "@tylertech/tyler-icons/standard": "@tylertech/tyler-icons/standard",
    "@tylertech/tyler-icons/extended": "@tylertech/tyler-icons/extended",
    "@tylertech/tyler-icons/custom": "@tylertech/tyler-icons/custom",
    "@tylertech/forge-core": "Forge.core",
    "imask": "IMask",
    "tslib": "tslib"
